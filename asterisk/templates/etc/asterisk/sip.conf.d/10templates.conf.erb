[ksp-local-phone](!)
nat = no
allow = ulaw
allow = gsm
deny = 0.0.0.0/0.0.0.0
permit = <%= network_eth0 %>/<%= netmask_eth0 %>

[ksp-local-hd-phone](!,ksp-local-phone)
allow = g722

[ksp-itsp](!)
allow = ulaw
allow = gsm

[ksp-hd-itsp](!,ksp-itsp)
allow = g722

[ksp-itsp-flowroute](!)
insecure = port,invite
