# Create top-level object in domain
dn: <%= ldap_domain %>
objectClass: top
objectClass: dcObject
objectclass: organization
o: <%= ldap_organization %>
dc: <%= ldap_dc %>
description: <%= ldap_description %>

# Admin user.
dn: cn=<%= ldap_rootdn %>,<%= ldap_domain %>
objectClass: simpleSecurityObject
objectClass: organizationalRole
cn: <%= ldap_rootdn %>
description: <%= ldap_admin_description %>
userPassword: <%= ldap_admin_passwd %>

dn: ou=people,<%= ldap_domain %>
objectClass: organizationalUnit
ou: people

dn: ou=groups,<%= ldap_domain %>
objectClass: organizationalUnit
ou: groups
